{
    "nodom": {
        "prefix": "ncm",
        "body": [
            "import {Module} from '${1:../js/nodom.js}'",
            "",
            "export class ${2} extends Module {",
            "",
            "   data = {",
            "       ${3}",
            "   }",
            "   methods = {",
            "       ${4}",
            "   }",
            "   template () {",
            "       return `",
            "       ${5}    ",
            "       `", 
            "   }",
            "   css = [{",
            "       ${6}",
            "   }]",
            "",
            "}"
    ]
},
"nodomFunction": {
    "prefix": "ndf",
    "body": [
        "${1:FunctionName}:function(dom,module,e,el){",
        "   ${2}",
        "},"
    ],
    "description": "生成nodom模板函数代码"
},
