{
    "nodom": {
        "prefix": "ncm",
        "body": [
            "import {Module} from '${1:../js/nodom.js}'",
            "",
            "export default class ${2} extends Module {",
            "",
            "   data() {",
            "     return {",
            "      ${3}",
            "     }",
            "   }",
            "   template() {",
            "       return `",
            "       ${4}",
            "       `", 
            "   }",
            "}"
    ]
},
"nodomFunction": {
    "prefix": "ndf",
    "body": [
        "${1:FunctionName}(model,dom,NEvent,e){",
        "   ${2}",
        "},"
    ],
    "description": "生成nodom模板函数代码"
},
